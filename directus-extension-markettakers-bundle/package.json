{
	"name": "markettakers",
	"description": "Please enter a description for your extension",
	"icon": "extension",
	"version": "1.0.0",
	"keywords": [
		"directus",
		"directus-extension",
		"directus-extension-bundle"
	],
	"type": "module",
	"files": [
		"dist"
	],
	"directus:extension": {
		"type": "bundle",
		"path": {
			"app": "dist/app.js",
			"api": "dist/api.js"
		},
		"entries": [
			{
				"type": "hook",
				"name": "oidc-role-map",
				"source": "src/oidc-role-map/index.ts"
			},
			{
				"type": "endpoint",
				"name": "steam",
				"source": "src/steam/index.ts"
			}
		],
		"host": "^10.10.0"
	},
	"scripts": {
		"build": "directus-extension build",
		"dev": "directus-extension build -w --no-minify",
		"link": "directus-extension link",
		"validate": "directus-extension validate",
		"add": "directus-extension add"
	},
	"devDependencies": {
		"@directus/extensions-sdk": "^16.0.1",
		"@types/node": "^24.3.0",
		"typescript": "^5.9.2"
	},
	"dependencies": {
		"@directus/errors": "^2.0.3",
		"express": "^4.21.2",
		"steam-totp": "^2.1.2"
	}
}
